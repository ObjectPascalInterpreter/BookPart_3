
// -> x1 -> x2 ->

vo = 0.5
k1 = 0.1; k2 = 0.14
function ode (x, df)
   global vo, k1, k2
   df[0] = vo - k1*x[0]
   df[1] = k1*x[0] - k2*x[1]
   return df
end

hstep = 0.1
x = [0, 0]; df = [0, 0]
t = 0
for i = 0 to 80 do
    ode (x, df)
    x[0] = x[0] + hstep*df[0]
    x[1] = x[1] + hstep*df[1]
    t = t + hstep
    println (strings.format (int (10*t)/10, "%f"),
              ", ", strings.format (x[0], "%5.4f"), 
              ", ", strings.format (x[1], "%5.6f"))
end



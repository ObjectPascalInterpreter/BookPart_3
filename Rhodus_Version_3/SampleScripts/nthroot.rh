function NthRoot(A, precision, n)
   x_p = math.sqrt(A)
   while math.abs(A - x_p^n) > precision do
      x = (1/n) * (((n-1) * x_p) + A/(x_p^(n - 1)))
      x_p = x
   end
   return x_p
end

print ("Enter the root power (eg 1, 2,3,4...): ")
n = int (readNumber())
x= 25
println ("Nthroot of ", x, " is: ", NthRoot (x, 1E-6, n)

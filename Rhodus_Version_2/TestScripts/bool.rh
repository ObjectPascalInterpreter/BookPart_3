x = 1; y1 = 6; w = 3; v = 4;
assertTrue  (True == True);
assertTrue  (True != False);
assertFalse (True != True);
assertFalse (False != False);
assertTrue  (not False);
assertFalse (not True);
assertTrue  (not not True);
assertTrue  (True or True);
assertTrue  (True or False);
assertTrue  (False or True);
assertTrue  (not not True);
assertFalse (False and True);

assertTrue  (True and True);
assertFalse (False and False);
assertFalse (not (False or True));
assertTrue  (not (False or False));
assertTrue  (not (False and True));
assertTrue  (not (True and False));

assertTrue (1 < 2);
assertTrue (1 <= 2);
assertTrue (not (1 > 2));
assertTrue (not (1 >= 2));
assertTrue (not (1 == 2));
assertTrue (1 != 2);

x = 1; y1 = 6; w = 3; v = 4;
assertTrue (False xor True and True == True);
assertTrue ((x < x and x > x) == False);
assertTrue (((x >= y1) and not (w <= v)) == False);
assertTrue ((y1 == 6) == True);

x = 1.2;
assertFalse (x == 3.4);
x = 3.4;

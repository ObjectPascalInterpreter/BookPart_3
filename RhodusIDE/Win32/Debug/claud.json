nature":"inv(matrix)","description":"Compute inverse of a matrix.","examples":["m = mat.inv (m)"]},{"methodType":"hmtMethod","methodName":"det","signature":"det(matrix)","description":"Compute the deteminant of a matrix.","examples":["d = mat.det (m)"]},{"methodType":"hmtMethod","methodName":"tr","signature":"tr(matrix)","description":"Compute the transpose of a matrix.","examples":["q = mat.tr (m)"]},{"methodType":"hmtMethod","methodName":"lu","signature":"lu(matrix)","description":"Compute the LU decompositon of a matrix.","examples":["[L, U, P] = mat.lu (m)"]},{"methodType":"hmtMethod","methodName":"qr","signature":"qr(matrix)","description":"Compute the QR decompositon of a matrix.","examples":["[Q, R] = mat.qr (m)"]},{"methodType":"hmtMethod","methodName":"rref","signature":"rref(matrix)","description":"Compute the reduced row echelon of a matrix.","examples":["q = mat.rref (m)"]},{"methodType":"hmtMethod","methodName":"solve","signature":"solve(A, b)","description":"Solve: x = mat.solve (m, b). b must be a column matrix.","examples":["x = mat.solve ({{1,2},{3,4}}, {{5},{7}})"]}]},{"recordType":"rtModule","moduleName":"lists","description":"This is the lists module","methods":[{"methodType":"hmtMethod","methodName":"range","signature":"range(start, finish, stepsize)","description":"Create a list based on the range arguments","examples":["l = lists.range (0, 10, 0.5)"]},{"methodType":"hmtMethod","methodName":"rndu","signature":"rndu(integer)","description":"Create a list of uniformly randomly distributed floats between 0 and 1. ","examples":["l = lists.rndu (10)"]},{"methodType":"hmtMethod","methodName":"rndi","signature":"rndi(lower : integer, upper : integer, number of elements : integer)","description":"Create a list of random uniformly distributed integers.","examples":["l = lists.rndi (lower, upper, number)","l = lists.rndi (0, 10, 5)","l = lists.rndi (5, 10, 20)"]}]},{"recordType":"rtDataObject","moduleName":"MatrixObject","description":"This is the matrix object","methods":[{"methodType":"hmtMethod","methodName":"rows","signature":"m.rows ()","description":"Get the number of rows in the matrix: var.rows()","examples":["n = m.rows()","println (p.rows())"]},{"methodType":"hmtMethod","methodName":"cols","signature":"m.cols ()","description":"Get the number of cols in the matrix: var.cols()","examples":["n = m.cols()","println (p.cols())"]},{"methodType":"hmtMethod","methodName":"shape","signature":"m.shape ()","description":"Get the shape of the matrix, that is rows by columns","examples":["[nrows, ncols] = m.shape()","println (p.shape())"]},{"methodType":"hmtMethod","methodName":"row","signature":"m.row (int)","description":"Extract a column from the matrix","examples":["col = m.col(1)"]},{"methodType":"hmtMethod","methodName":"col","signature":"m.col (int)","description":"Extract a row from the matrix","examples":["row = m.row(2)"]},{"methodType":"hmtMethod","methodName":"toArray","signature":"m.toArray ()","description":"Convert a matrix to an array.","examples":["a = m.toArray(m)"]},{"methodType":"hmtMethod","methodName":"toList","signature":"m.toList ()","description":"Convert a matrix to a list.","examples":["l = m.row()"]}]}]}
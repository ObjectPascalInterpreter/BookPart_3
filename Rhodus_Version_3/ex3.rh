
a = random.randlist (30, 30)

found = -1
// Check if list cotnains the number 5
for i = 0 to lists.len (a) - 1 do
    if a[i] == 5 then
       found = i
       break    
    end
end

if found != -1 then
   println ("Found element at: ", found)
else
   println ("Could not find an element 5 in the list")
end
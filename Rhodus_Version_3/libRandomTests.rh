

length = 100000
a = random.randlist (length, 10)

// Find frequency of numbers in random list

freq = {0} * 10

for i = 0 to length - 1 do
    freq[a[i]] = freq[a[i]] + 1
end

for i = 0 to 10 - 1 do
   freq[i] = freq[i] / length
end

println (freq)




 
println ("Convert number to binary")

n = 176

// array to store binary number 
binaryNum = [0] * 16; 
  
// counter for binary array 
i = 0
while (n > 0) do  
   
   // storing remainder 
   // in binary array 
   binaryNum[i] = n mod 2; 
   n = int(n / 2) 
   i = i + 1 
end
  
// printing binary array 
// in reverse order 
for i = binaryNum.len () - 1 downto 0 do
    print(binaryNum[i])
end
println()